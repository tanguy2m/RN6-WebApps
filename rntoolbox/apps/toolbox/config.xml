<Application resource-id="toolbox"><!-- 'resource-id' must be AppName -->
  <Name>Toolbox</Name><!-- Any descriptive name, up to 48 chars -->
  <Author>tanguy2m</Author><!-- Author name. up to 48 chars -->
  <Version>0.1.0</Version><!-- Version -->
  <RequireReboot>0</RequireReboot><!-- If non-zero, a reboot is required. -->
  <ConfigURL/>
  <LaunchURL>https://localhost/apps/toolbox/</LaunchURL><!-- 'localhost' will be replaced by framework JS. -->
  <DebianPackage>rntoolbox</DebianPackage>
  <ServiceName/><!-- no service required -->
  <Description lang="en-us">Toolbox to manage ReadyNAS web apps</Description>
</Application>
